<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.PhoneTreeReportScreenController">
    <Label alignment="CENTER" layoutX="9.0" layoutY="14.0" prefHeight="53.0" prefWidth="465.0"
           text="Customer Phone Tree">
        <font>
          <Font name="System Bold" size="36.0"/>
        </font>
    </Label>
    <TableView fx:id="phoneTreeTableView" layoutX="12.0" layoutY="86.0" prefHeight="616.0" prefWidth="450.0">
      <columns>
          <TableColumn fx:id="customerNameCol" maxWidth="204.0" minWidth="-1.0" prefWidth="204.0" resizable="false"
                       text="Name"/>
          <TableColumn fx:id="customerPhoneCol" prefWidth="245.0" resizable="false" text="Phone Number"/>
      </columns>
    </TableView>
    <Button fx:id="returnToReportBtn" layoutX="172.0" layoutY="730.0" mnemonicParsing="false" onAction="#onActionReturn"
            text="Return to Reports Page"/>
</AnchorPane>

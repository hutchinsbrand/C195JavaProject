<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.ViewReportsScreenController">
   <Label layoutX="46.0" layoutY="14.0" text="View Reports">
      <font>
         <Font size="36.0"/>
      </font>
   </Label>
   <ComboBox fx:id="reportTypeComboBox" layoutX="26.0" layoutY="173.0" onAction="#onActionChangeType" prefHeight="25.0"
             prefWidth="250.0"/>
   <Label layoutX="58.0" layoutY="118.0" text="PLease Choose a Report Type:">
      <font>
         <Font size="14.0"/>
      </font>
   </Label>
   <Button fx:id="returnBtn" layoutX="74.0" layoutY="412.0" mnemonicParsing="false" onAction="#onActionReturn"
           text="Return to Home Page">
      <font>
         <Font size="14.0"/>
      </font>
   </Button>
</AnchorPane>
